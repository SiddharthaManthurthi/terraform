terraform {
  required_providers {
    aws = {
      source = "hashicorp/aws"
      version = "6.33.0" # Terraform AWS provider version
    }
  }

  backend "s3" {
    bucket         = "siddharthais.online.devops"
    key            = "localsdemo.tfstate"
    region         = "us-east-1"
    encrypt        = true
    use_lockfile    = true  # Enables native S3 locking
  }
}
provider "aws" {
  region = "us-east-1"
}


